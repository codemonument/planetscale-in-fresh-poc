{
  "tasks": {
    "start": "doppler run -- deno run --unstable -A --watch=static/,routes/ dev.ts",
    "prod": "doppler run -- deno run --unstable -A --watch=static/,routes/ main.ts",
    "reload-cache": "deno cache --reload --unstable  main.ts",
    "db-shell": "pscale shell jbscratch_planetscale-in-deno main",
    "db-minimal": "deno task db-base ./scripts/db/minimal-select.ts",
    "db-reset": "deno task db-base ./scripts/db/reset-pets.ts",
    "db-list": "deno task db-base ./scripts/db/list-pets.ts",
    "db-base": "doppler run -- deno run --unstable --allow-env=DATABASE_HOST,DATABASE_USERNAME,DATABASE_PASSWORD --allow-net=aws.connect.psdb.cloud "
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
